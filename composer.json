{
    "name": "fighcell/yii2-base",
    "description": "Improved Yii 2 Advanced Application Template By Nenad Zivkovic modified by fighcell",
    "keywords": ["yii2", "framework", "advanced", "improved", "application template", "nenad", "fighcell"],
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "tutorial": "http://www.freetuts.org/tutorial/view?id=6",
        "source": "https://fighcell@bitbucket.org/fighcell/yii2-base"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "^2.0.44",
        "yiisoft/yii2-bootstrap4": "^2.0.10",
        "demokn/yii2-composer-asset": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "nenad/yii2-password-strength": "*",
        "bower-asset/jquery": "2.1.*@stable | 1.11.*@stable",
        "bower-asset/yii2-pjax": "^2.0.7.1",
        "npm-asset/bootstrap": "^4.6.1",
        "npm-asset/popper": ">=0.2.6",
        "mihaildev/yii2-ckeditor": "*",
        "fighcell/yii2-user": "^0.9.14",
        "fighcell/yii2-rbac": "^1.0"
    },
    "require-dev": {
        "codeception/codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*",
        "codeception/specify": "*",
        "codeception/verify": "*",
        "cweagans/composer-patches": "^1.7",
        "phpunit/phpunit": "4.8.34"
    },
    "config": {
        "vendor-dir": "_protected/vendor",
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patches": {
            "phpunit/phpunit-mock-objects": {
                "Fix PHP 7 and 8 compatibility": "https://yiisoft.github.io/phpunit-patches/phpunit_mock_objects.patch"
            },
            "phpunit/phpunit": {
                "Fix PHP 7 compatibility": "https://yiisoft.github.io/phpunit-patches/phpunit_php7.patch",
                "Fix PHP 8 compatibility": "https://yiisoft.github.io/phpunit-patches/phpunit_php8.patch"
            }
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
